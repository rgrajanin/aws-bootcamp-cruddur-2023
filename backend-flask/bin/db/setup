#! /usr/bin/bash
set -e # stop if it fails at any point

CYAN='\033[1;36m'
NO_COLOR='\033[0m'
LABEL="setup"
printf "${CYAN}== ${LABEL}${NO_COLOR}\n"

echo "==== setup"

bin_path="$(realpath .)/bin/db"

source "$bin_path/drop"
source "$bin_path/create"
source "$bin_path/schema-load"
source "$bin_path/seed"
